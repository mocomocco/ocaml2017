#include<stdio.h>
#include<stdlib.h>

int gl,gl2;

int main(){
  int lo,lo2;
  static int st,st2;
  int stack[5],stack2[5];
  int *heap = malloc(sizeof(char)*5),*heap2=malloc(sizeof(char)*5);

  printf("mainのアドレス   ：%p\n\n",&main);
  printf("glのアドレス   　：%p\n",&gl);
  printf("gl2のアドレス    ：%p\n",&gl2);
  printf("stのアドレス 　　：%p\n",&st);
  printf("st2のアドレス 　 ：%p\n",&st2)
  printf("\n");
  printf("loのアドレス     ：%p\n",&lo);
  printf("lo2のアドレス　　：%p\n",&lo2);
  printf("stackのアドレス  ：%p\n",&stack);
  printf("stsck2のアドレス ：%p\n",&stack2);
  printf("\n");
  printf("heapのアドレス ：%p\n",heap);
  printf("heap2のアドレス：%p\n",heap2);
  printf("\n");
}
